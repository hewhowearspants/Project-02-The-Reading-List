<% include ../partials/boilerplate %>
<% include ../partials/nav {user: user} %>
<div class='container'>
  <div class='search-results-container'>
    <% for (let store of stores) { %>
    <div class='store'>
      <p><%= store.name %></p>
      <p><%= store.address %></p>
      <% let storeSearch = store.address.split().join('+'); %>
      <a href='https://www.google.com/maps/search/?api=1&query=<%= store.address %>&query_place_id=<%= store.placeId %>' target='_blank'>Google Maps</a>
    </div>
  <% } %>
  </div>
</div>
<% include ../partials/end %>